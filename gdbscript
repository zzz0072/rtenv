shell rm -f gdb.txt
file main.elf
target remote :3333
set debug timestamp on

b main
b kernel.c:61
c
set $ticks = &tick_count
b pathserver.c:25
b serial.c:14
b serial.c:37
b serial.c:54
b shell.c:205
commands 3 4 5 6 7
    echo tick:
    output (unsigned int) *($ticks)
    echo current:
    output {uint32_t} 0xE000e018
    echo countDown:
    output {uint32_t} 0xE000e014
	c
end
c
set logging on
